{
  "name": "server",
  "version": "1.0.0",
  "author": "Ian Landicho",
  "license": "MIT",
  "description": "Medication Admin Record for Family Use API",
  "main": "build/server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"yarn compile:watch\" \"yarn dev:server\"",
    "compile:watch": "tsc --watch",
    "dev:server": "nodemon build/server.js",
    "xpostinstall": "npm run compile",
    "compile": "tsc",
    "start": "node build/server.js",
    "test:watch": "jest --no-cache --runInBand --watch",
    "test:debug": "ndb jest --watch --no-cache --runInBand --watch",
    "test": "jest --no-cache --runInBand",
    "lint": "eslint './src/*.{js,ts}' --quiet --fix",
    "deploy": "git push marfu-api master"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.18",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mongoose": "^5.10.3",
    "ts-jest": "^26.5.2"
  }
}
